.data
    quebra_l: .asciiz "\n"

.text
main:

    #Inserir 1o numero
    li $v0, 5
    syscall

    #Armazenar 1o numero em $t0
    move $t0, $v0

    #inserir 2o numero
    li $v0, 5
    syscall

    #Armazenar o 2o numero em $t1
    move $t1, $v0

    #somar e mostrar resultado
    sub $t2, $t0, $t1
    li $v0, 1
    move $a0, $t2
    syscall

    #Quebra de linha
    li $v0, 4
    la $a0, quebra_l
    syscall

    #Encerrar
    li $v0, 10
    syscall